{
  "gentypeconfig": {
    "language": "typescript",
    "module": "es6",
    "moduleResolution": "node16",
    "importPath": "relative",
    "shims": {
      "Js": "Js"
    },
    "debug": {
      "all": false
    },
    "exportInterfaces": false
  },
  "name": "sample-typescript-app",
  "bsc-flags": ["-bs-super-errors"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js"
}
