  "    packages.parse_packages",
  "    clean.cleanup_previous_build",
  "  incremental_build[module_count=3]",
  "    build.parse[dirty_modules=2]",
  "      build.parse_file[module=Consumer-TestNS, package=@rewatch-test/namespaced]",
