{
  "gentypeconfig": {
    "language": "typescript",
    "module": "esmodule",
    "importPath": "relative",
    "shims": {
      "Js": "Js",
      "Jsx": "Jsx",
      "JsxEvent": "JsxEvent",
      "Belt": "Belt",
      "RescriptPervasives": "RescriptPervasives"
    },
    "debug": {
      "all": false
    },
    "exportInterfaces": false
  },
  "name": "sample-typescript-app",
  "bsc-flags": [],
  "jsx": { "version": 4 },
  "bs-dependencies": ["@rescript/react"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "uncurried": true,
  "package-specs": {
    "module": "esmodule",
    "in-source": true
  },
  "suffix": ".res.js"
}
