{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ESNext",
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "Node",
    "allowJs": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "types": []
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/**/*.d.ts"],
  "exclude": ["node_modules"]
}

